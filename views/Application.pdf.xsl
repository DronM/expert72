<?xml version="1.0" encoding="UTF-8"?><xsl:stylesheet version="1.0"  xmlns:html="http://www.w3.org/TR/REC-html40" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:fo="http://www.w3.org/1999/XSL/Format"><xsl:output method="xml"/> <!--https://www.webucator.com/tutorial/learn-xsl-fo--><!-- Main template --><xsl:template match="/">	<fo:root>		<fo:layout-master-set>			<fo:simple-page-master master-name="Report"				page-height="21cm" page-width="14.7cm" margin-top="0.5cm"				margin-left="1cm" margin-right="0.5cm" margin-bottom="0.5cm">				<fo:region-body margin-bottom="0.5cm"/>				<fo:region-before/>				<fo:region-after extent=".3cm" background-color="silver"/>			</fo:simple-page-master>		</fo:layout-master-set>		<fo:page-sequence master-reference="Report">	  			<fo:static-content flow-name="xsl-region-after">				<fo:block font-family="Arial" font-size="6pt" text-align="right">					Страница <fo:page-number/> из <fo:page-number-citation ref-id="last-page"/>				</fo:block>			</fo:static-content>					<fo:flow flow-name="xsl-region-body">						<xsl:apply-templates select="document/model"/>					<fo:block id="last-page"/>			</fo:flow>							</fo:page-sequence>	</fo:root></xsl:template><xsl:template match="model[@id='ApplicationPrint_Model']">	<fo:block-container font-family="Arial" font-style="normal">		<fo:table table-layout="fixed">			<fo:table-column column-width="50%"/>			<fo:table-column column-width="50%"/>						<fo:table-body font-size="8pt">				<fo:table-row height="0.4cm">					<fo:table-cell>						<fo:block>						</fo:block>					</fo:table-cell>																																	<fo:table-cell>						<fo:block font-family="Arial" font-style="normal"							font-weight="normal" text-align="right">							<xsl:value-of select="concat(row/boss_post_dat,' ',row/office_rod,' ',row/boss_name_dat)"/>						</fo:block>					</fo:table-cell>				</fo:table-row>			</fo:table-body>		</fo:table>						<fo:block font-family="Arial" font-style="normal"			margin-top="15px" font-size="14px"			font-weight="bold" text-align="center">			Заявление № <xsl:value-of select="row/id"/> от <xsl:value-of select="row/date_descr"/>		</fo:block>		<fo:block font-family="Arial" font-style="normal"			margin-top="15px" font-size="8px" text-indent="1cm"			text-align="left">			<xsl:call-template name="doc_head"/>			<!--			Просим Вас провести государственную экспертизу проектной документации <xsl:if test="row/expertise_type!='pd'"> и результатов инженерных изысканий </xsl:if>			-->			<fo:inline font-family="Arial" font-weight="bold" font-size="8px" text-decoration="underline">				<xsl:value-of select="row/constr_name"/>, по адресу: <xsl:value-of select="row/constr_address"/>			</fo:inline>								</fo:block>		<fo:table table-layout="fixed" margin-left="5px" margin-top="2px">			<fo:table-column column-width="50%"/>			<fo:table-column column-width="50%"/>						<fo:table-body font-size="8pt">				<fo:table-row height="0.4cm" background-color="Gainsboro">					<fo:table-cell>						<fo:block font-family="Arial" font-style="normal"							font-weight="normal" text-align="left">							Вид объекта капитального строительства:						</fo:block>					</fo:table-cell>																																	<fo:table-cell>						<fo:block font-family="Arial" font-style="normal"							font-weight="normal" text-align="left">							<xsl:value-of select="row/construction_types_descr"/>						</fo:block>					</fo:table-cell>				</fo:table-row>				<fo:table-row height="0.4cm" background-color="white">					<fo:table-cell>						<fo:block font-family="Arial" font-style="normal"							font-weight="normal" text-align="left">							Источник финансирования:						</fo:block>					</fo:table-cell>																																	<fo:table-cell>						<fo:block font-family="Arial" font-style="normal"							font-weight="normal" text-align="left">							<xsl:value-of select="row/fund_sources_descr"/>						</fo:block>					</fo:table-cell>				</fo:table-row>								<xsl:apply-templates select="row/constr_technical_features"/>				</fo:table-body>		</fo:table>			<!-- Сведения о заявителе -->		<fo:block font-family="Arial" font-style="normal"			margin-top="10px" font-size="10pt"			font-weight="bold">			Сведения о заявителе		</fo:block>		<fo:table table-layout="fixed" margin-left="5px">			<fo:table-column column-width="50%"/>			<fo:table-column column-width="50%"/>						<fo:table-body font-size="8pt">				<xsl:apply-templates select="row/applicant"/>				</fo:table-body>		</fo:table>		<!-- Сведения о заказчике -->		<fo:block font-family="Arial" font-style="normal"			margin-top="10px" font-size="10pt"			font-weight="bold" margin-left="5px">			Сведения о заказчике		</fo:block>		<fo:table table-layout="fixed" margin-left="5px">			<fo:table-column column-width="50%"/>			<fo:table-column column-width="50%"/>						<fo:table-body font-size="8pt">				<xsl:apply-templates select="row/customer"/>				</fo:table-body>		</fo:table>		<!-- Сведения об исполнителях -->		<fo:block font-family="Arial" font-style="normal"			margin-top="10px" font-size="10pt"			font-weight="bold" margin-left="5px">			Сведения об исполнителях работ		</fo:block>		<xsl:apply-templates select="row/contractors"/>			<fo:block font-family="Arial" font-style="normal"			margin-top="10px" font-size="12pt"			font-weight="bold" margin-left="5px">			Приложения		</fo:block>		<!-- Pd AND EngSurvey Files -->		<xsl:apply-templates select="row/documents_pd_eng_survey"/>				<!-- CostEvalValidity Files -->		<xsl:if test="row/cost_eval_validity='t'">				<!--			<fo:block font-family="Arial" font-style="normal"				margin-top="2px" font-size="10pt"				font-weight="bold" margin-left="5px">				Файлы по достоверености			</fo:block>			-->			<xsl:apply-templates select="row/documents_cost_eval_validity"/>		</xsl:if>				<xsl:call-template name="doc_foot"/>	</fo:block-container></xsl:template><xsl:template match="documents_pd_eng_survey">	<fo:block font-family="Arial" font-style="normal"		margin-top="10px" font-size="10pt"		font-weight="bold" margin-left="5px">		Файлы проектной документации и результатов инженерных изысканий:	</fo:block>	<xsl:apply-templates select="files"/></xsl:template><xsl:template match="documents_cost_eval_validity">	<fo:block font-family="Arial" font-style="normal"		margin-top="10px" font-size="10pt"		font-weight="bold" margin-left="5px">		Файлы по проверке достоверености сметной стоимости:	</fo:block>	<xsl:apply-templates select="files"/></xsl:template><xsl:template match="contractors">	<fo:table table-layout="fixed" margin-top="5px" margin-left="5px">		<fo:table-column column-width="50%"/>		<fo:table-column column-width="50%"/>			<fo:table-body font-size="8pt">			<xsl:apply-templates select="contractor"/>			</fo:table-body>	</fo:table></xsl:template><xsl:template match="feature">	<xsl:variable name="color">		<xsl:choose>		<xsl:when test="(position()+1) mod 2 = 0">Gainsboro</xsl:when>		<xsl:otherwise>white</xsl:otherwise>		</xsl:choose>	</xsl:variable>	<fo:table-row height="0.4cm" background-color="{$color}">		<fo:table-cell>			<fo:block font-family="Arial" font-style="normal"				font-weight="normal" text-align="left">				<xsl:value-of select="@name"/>			</fo:block>		</fo:table-cell>																														<fo:table-cell>			<fo:block font-family="Arial" font-style="normal"				font-weight="normal" text-align="left">				<xsl:value-of select="@value"/>			</fo:block>		</fo:table-cell>	</fo:table-row></xsl:template><xsl:template match="field">	<xsl:variable name="color">		<xsl:choose>		<xsl:when test="position() mod 2 = 0">white</xsl:when>		<xsl:otherwise>Gainsboro</xsl:otherwise>		</xsl:choose>	</xsl:variable>	<fo:table-row height="0.4cm" background-color="{$color}">		<fo:table-cell>			<fo:block font-family="Arial" font-style="normal"				font-weight="normal" text-align="left">				<xsl:value-of select="@id"/>			</fo:block>		</fo:table-cell>																														<fo:table-cell>			<fo:block font-family="Arial" font-style="normal"				font-weight="normal" text-align="left">				<xsl:value-of select="node()"/>			</fo:block>		</fo:table-cell>	</fo:table-row></xsl:template><xsl:template match="file">	<xsl:variable name="color">		<xsl:choose>		<xsl:when test="position() mod 2 = 0">white</xsl:when>		<xsl:otherwise>Gainsboro</xsl:otherwise>		</xsl:choose>	</xsl:variable>	<fo:table-row height="0.4cm" background-color="{$color}">		<fo:table-cell>			<fo:block font-family="Arial" font-style="normal"				font-weight="normal" text-align="left">				<xsl:value-of select="@path"/>			</fo:block>		</fo:table-cell>																														<fo:table-cell>			<fo:block font-family="Arial" font-style="normal"				font-weight="normal" text-align="left">				<xsl:value-of select="@name"/>			</fo:block>		</fo:table-cell>			</fo:table-row></xsl:template><xsl:template match="files">	<fo:table table-layout="fixed" margin-left="5px">		<fo:table-column column-width="65%"/>		<fo:table-column column-width="35%"/>		<fo:table-body font-size="8pt">			<xsl:apply-templates/>		</fo:table-body>	</fo:table>		</xsl:template><xsl:template name="doc_head">	<xsl:text>Просим Вас провести государственную экспертизу проектной документации </xsl:text><xsl:if test="row/expertise_type!='pd'"> и результатов инженерных изысканий </xsl:if></xsl:template><xsl:template name="doc_foot"></xsl:template></xsl:stylesheet>